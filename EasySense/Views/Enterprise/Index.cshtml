        <table style="width:600px">
            <tr>
                <td>企业名称</td>
                <td><input type="text" class="textbox" placeholder="企业名称" /></td>
                <td>企业等级</td>
                <td>
                    <select class="select span-width">
                        <option value="0">A</option>
                        <option value="1">B</option>
                        <option value="2">C</option>
                        <option value="3">D</option>
                    </select>
                </td>
            </tr>
